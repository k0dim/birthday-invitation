# Telegram Bot Configuration
# Получите токен у @BotFather в Telegram
VITE_TELEGRAM_BOT_TOKEN=your_bot_token_here

# Получите ваш Chat ID у @getmyid_bot в Telegram
VITE_TELEGRAM_CHAT_ID=your_chat_id_here

# Режим работы (development/production)
NODE_ENV=development

# Базовый URL для приложения
VITE_BASE_URL=/

# Домен приложения (для ссылок)
VITE_APP_DOMAIN=birthday.your-domain.com

# Отладка (true/false)
VITE_DEBUG=false

# Время ожидания Telegram API (мс)
VITE_TELEGRAM_TIMEOUT=10000
